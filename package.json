{
  "name": "booker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "npx prettier . -c -w",
    "test": "npm run test:component && npm run test:page && npm run test:db",
    "test:component": "npx jest ./src/components/",
    "test:page": "npx jest ./src/__test__",
    "test:db": "npx jest --testTimeout=50000 --forceExit --config=./jest.config.node.ts ./tests/db/ ",
    "server": "ts-node-esm --esm -P ./tsconfig.script.json ./server/index.ts",
    "server:dev": "nodemon",
    "server:build": "NODE_ENV=production npm run server",
    "seed": "ts-node -P ./tsconfig.script.json ./scripts/seed.ts"
  },
  "dependencies": {
    "@next/font": "13.1.2",
    "@reduxjs/toolkit": "^1.9.1",
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-graphql": "^0.12.0",
    "graphql": "^16.6.0",
    "mariadb": "^3.0.2",
    "next": "13.1.2",
    "nodemailer": "^6.9.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "redis": "^4.5.1",
    "showdown": "^2.1.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.15",
    "@types/express-fileupload": "^1.4.1",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@types/nodemailer": "^6.4.7",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/showdown": "^2.0.0",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-transform-css": "^6.0.1",
    "next-router-mock": "^0.9.1-beta.0",
    "node-fetch": "^2.6.9",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "author": {
    "email": "siamhb7@protonmail.com",
    "name": "syaw0"
  },
  "nodemonConfig": {
    "watch": [
      "./server/**/*"
    ],
    "ext": ".ts",
    "exec": "npm run server"
  }
}
